{
  "buildCommand": "npx quartz build",
  "outputDirectory": "public",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/edit",
      "destination": "/static/edit.html"
    },
    {
      "source": "/admin",
      "destination": "/static/edit.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/sheets/(.*)",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
