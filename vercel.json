{
  "buildCommand": "npx quartz build",
  "outputDirectory": "public",
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/edit",
      "destination": "/static/edit.html",
      "permanent": false
    },
    {
      "source": "/admin",
      "destination": "/static/edit.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/assets/sheets/(.*)",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
